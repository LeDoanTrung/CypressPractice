{
  "name": "cypress_project",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "dev": "cypress open",
    "test": "cypress run --browser chrome --headed",
    "test:mocha-reporter": "cypress run --reporter mochawesome --reporter-options reportDir=\"results\",overwrite=false,html=false,json=true",
    "merge-reports": "mochawesome-merge \"cypress/results/*.json\" > mochawesome.json",
    "build-report": "marge mochawesome.json",
    "open-report": "serve mochawesome-report"
  },
  "author": "TrungLe",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "cypress": "^13.13.3",
    "cypress-xpath": "^2.0.1"
  },
  "devDependencies": {
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.3.0",
    "mochawesome-report-generator": "^6.2.0",
    "marge" :"1.0.1",
    "serve" : "14.1.2"
  }
}
